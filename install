HERE=$(dirname $(grealpath $0))
[ "$1" == ""  ] && SHELL=${HERE}/repos/shell
[ "$1" == ""  ] || SHELL=$1
DOT=${HERE}/dot
rm -rf $DOT 2> /dev/null
mkdir -p $DOT
echo "source ${SHELL}/dot.zprofile.sh " > ${DOT}/zprofile
echo "source ${SHELL}/dot.profile.sh  " > ${DOT}/profile
echo "source ${SHELL}/dot.bashrc.sh   " > ${DOT}/bashrc
git clone https://github.com/bryanhann/bch.shell ${SHELL}
